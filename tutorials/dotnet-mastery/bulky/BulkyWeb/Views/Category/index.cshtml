@model List<CategoryModel>

<h1>Category List</h1>

<table>
  <thead>
    <tr>
      <th>Category Name</th>
      <th>Display Order</th>
    </tr>
  </thead>
  <tbody>
    @foreach(var category in Model)
    {
      <tr>
        <td>@category.Name</td>
        <td>@category.DisplayOrder</td>
      </tr>
    }
  </tbody>
</table>
