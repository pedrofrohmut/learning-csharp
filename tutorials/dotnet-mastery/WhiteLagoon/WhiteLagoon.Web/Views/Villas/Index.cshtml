@model IEnumerable<WhiteLagoon.Domain.Entities.Villa>

<h1>Villa List</h1>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <td>Name</td>
      <td>Price</td>
      <td>Sqft</td>
      <td>Occupancy</td>
    </tr>
  </thead>
  <tbody>
    @foreach (var villa in Model)
    {
      <tr>
        <td>@villa.Name</td>
        <td>@villa.Price</td>
        <td>@villa.Sqft</td>
        <td>@villa.Occupancy</td>
      </tr>
    }
  </tbody>
</table>
